        "eslint": "^8.0.1",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "husky": "^8.0.0",
        "lint-staged": "^15.0.0",
        "prettier": "^3.0.0",
        "typescript": "^5.8.2"
    },
    "lint-staged": {
        "*.ts": [
